{
  "name": "ob-scene",
  "version": "0.2.2",
  "description": "A reactive animation engine. Create interactive infographics with parallax, autoplay, & slides capabilities.",
  "keywords": [
    "animation",
    "reactive",
    "observable"
  ],
  "main": "index.js",
  "scripts": {
    "test": "",
    "build": "./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --watch",
    "minify": "NODE_ENV='production' npm run build",
    "major": "npm run minify && ./node_modules/.bin/bump major",
    "minor": "npm run minify && ./node_modules/.bin/bump minor",
    "patch": "npm run minify && ./node_modules/.bin/bump patch"
  },
  "author": "Sartaj <sartaj@sartaj.me> (http://sartaj.me)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sartaj/ob-scene.git"
  },
  "bin": {
    "obscene": "./compiler/cli.js"
  },
  "dependencies": {
    "howler": "^1.1.29",
    "kefir": "^3.2.0",
    "pace": "HubSpot/pace"
  },
  "devDependencies": {
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "colors": "^1.1.2",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "html-loader": "latest",
    "html2hscript": "^1.2.2",
    "json-loader": "latest",
    "on-build-webpack": "^0.1.0",
    "watch": "^0.17.1"
  }
}
